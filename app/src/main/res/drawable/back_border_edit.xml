<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape>
            <gradient
                android:startColor="#9AFDFDFD"
                android:endColor="#A8FFFFFD"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#828486" />
            <corners
                android:radius="10dp" />
            <padding
                android:left="5dp"
                android:top="1dp"
                android:right="5dp"
                android:bottom="1dp" />
        </shape>
    </item>
</selector>